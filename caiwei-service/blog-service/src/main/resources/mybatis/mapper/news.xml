<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="xyz.lius.blog.mapper.NewsMapper">
    <resultMap id="news" type="xyz.lius.blog.model.News">
    </resultMap>

    <select id="findList" resultMap="news">
        select * from news order by news_id limit 10;
    </select>
</mapper>
